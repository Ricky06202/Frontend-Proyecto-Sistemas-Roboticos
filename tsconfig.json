{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"],
			"@components/*": ["src/components/*"],
			"@pages/*": ["src/pages/*"],
			"@modules/*": ["src/modules/*"],
			"@attendance/*": ["src/modules/attendance/*"],
			"@authentication/*": ["src/modules/authentication/*"],
			"@landingPage/*": ["src/modules/landingPage/*"]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "react"
	}
}
